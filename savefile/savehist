;; -*- mode: emacs-lisp; coding: utf-8-unix -*-
;; Minibuffer history file, automatically generated by ‘savehist’.

(setq savehist-minibuffer-history-variables '(buffer-name-history extended-command-history file-name-history))
(setq buffer-name-history '("init-base-config.el"))
(setq file-name-history '("~/.emacs.d/init-base-config.el" "~/.emacs.d/" "~/.emacs.d/modules" "~/.emacs.d/"))
(setq search-ring 'nil)
(setq regexp-search-ring 'nil)
